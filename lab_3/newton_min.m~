function res = newton_min(func, grad, hess, x0)
    prev_x = x0;
    x = x0 + 1;
    n = 0;
    while norm(prev_x - x) > 0.01
        norm(prev_x - x)
        n = n + 1;
        prev_x = x;
        x = x - inv(hess(x) \ grad(x);
    end
    disp(['Number of iterations: ', num2str(n)]);
    res = x;
end